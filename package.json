{
  "name": "goanflow",
  "version": "1.0.0",
  "description": "AI-native tourism operating system for Goa using 6-agent orchestration",
  "main": "dist/orchestrator.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/orchestrator.ts",
    "server": "node --max-http-header-size=16000 -r ts-node/register src/server.ts",
    "client": "cd client && npm start",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:agents": "jest --testPathPattern=agents"
  },
  "keywords": [
    "ai",
    "tourism",
    "goa",
    "agents",
    "kiro",
    "bedrock",
    "amplify"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.958.0",
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-sagemaker-runtime": "^3.490.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "@types/pg": "^8.16.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "groq-sdk": "^0.8.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}

